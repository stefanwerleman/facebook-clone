{
   "name": "facebook-clone-api",
   "version": 2,
   "builds":
   [
      {
         "src": "index.js",
         "use": "@now/node-server"
      }
   ],
   "routes":
   [
      {
         "src": "/.*",
         "dest": "/index.js"
      },
      {
         "src": "/posts",
         "dest": "/index.js",
         "methods": ["GET"]
      }
   ],
   "env":
   {
      "MONGODB_URI": "@facebook-clone-uri"
   }
}
